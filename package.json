    "redux-devtools-extension": "^2.13.8",
    "style-loader": "^2.0.0",
    "stylus-loader": "^4.3.2",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^5.12.1",
    "webpack-cli": "^4.3.1",
    "webpack-dev-server": "^3.11.1",
    "webpack-node-externals": "^2.5.2",
